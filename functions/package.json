{
  "name": "functions",
  "scripts": {
    "correct": "yarn run prettier --write ./src/**/*",
    "build": "yarn run tslint -p tslint.json && yarn run tsc && yarn correct",
    "serve": "npm run build && firebase serve --only functions",
    "shell": "npm run build && firebase experimental:functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "main": "lib/index.js",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-context": "^1.0.3",
    "apollo-link-http": "^1.3.2",
    "axios": "^0.17.1",
    "firebase": "5.5.8",
    "firebase-admin": "^6.1.0",
    "firebase-functions": "^2.1.0",
    "firebase-tools": "^6.0.1",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "luxon": "^0.2.11",
    "node-fetch": "^1.7.3"
  },
  "devDependencies": {
    "@types/luxon": "^0.2.2",
    "prettier": "^1.9.2",
    "tslint": "^5.8.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.5.3"
  },
  "private": true
}
